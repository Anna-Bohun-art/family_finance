{
  "name": "family_finance",
  "version": "1.0.0",
  "description": "A small app for managing family finance",
  "author": "Anna",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "start": "npm run dev",
    "serve": "vue-cli-service serve",
    "lint": "eslint --ext .js,.vue src",
    "build": "node build/build.js",
    "lint:fix": "vue-cli-service lint",
    "install:clean": "rm -rf node_modules && rm -rf package-lock.json && npm install"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.15.3",
    "@popperjs/core": "2.9.1",
    "@tailwindcss/forms": "0.2.1",
    "@vue/compiler-sfc": "3.0.7",
    "@vueform/multiselect": "^1.5.0",
    "@vuelidate/core": "^2.0.0-alpha.18",
    "@vuelidate/validators": "^2.0.0-alpha.15",
    "axios": "^0.21.1",
    "chart.js": "2.9.3",
    "core-js": "3.9.1",
    "fuzzaldrin-plus": "^0.6.0",
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.8",
    "tailwindcss": "2.0.4",
    "vue": "3.0.7",
    "vue-axios": "^3.2.4",
    "vue-multiselect": "^2.1.6",
    "vue-resource": "^1.5.2",
    "vue-router": "4.0.5"
  },
  "devDependencies": {
    "@babel/core": "7.13.10",
    "@babel/eslint-parser": "7.13.10",
    "@vue/cli-plugin-babel": "5.0.0-beta.2",
    "@vue/cli-plugin-eslint": "5.0.0-beta.2",
    "@vue/cli-service": "^5.0.0-beta.2",
    "autoprefixer": "10.2.5",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-base": "^11.3.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-vue": "7.7.0",
    "postcss": "^8.3.2",
    "vue-template-compiler": "2.6.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "main": ".postcssrc.js",
  "directories": {
    "test": "test"
  },
  "license": "ISC"
}
